{
  "env": {
    "MCP_SERVER_URL": "https://airbnb-mcp-production.up.railway.app",
    "CHROMIUM_REMOTE_EXEC_PATH": "https://github.com/Sparticuz/chromium/releases/download/v133.0.0/chromium-v133.0.0-pack.tar.br"
  },
  "functions": {
    "api/**.{js,ts,cjs,mjs}": {
      "maxDuration": 30,
      "memory": 1024,
      "includeFiles": [
        "node_modules/@sparticuz/**",
        "node_modules/puppeteer-core/**",
        "node_modules/follow-redirects/**",
        "node_modules/streamx/**",
        "node_modules/fast-fifo/**"
      ]
    }
  }
}